{
  "name": "my-inventory-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "check-format": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:db": "ts-node --esm scripts/test-db-connection.ts",
    "test:api": "ts-node --project tsconfig.node.json scripts/test-api-endpoints.ts",
    "test:dynamic": "ts-node --project tsconfig.node.json scripts/test-dynamic-routes.ts",
    "test:inventory": "ts-node --project tsconfig.node.json scripts/test-inventory-api.ts",
    "test:auth": "ts-node --project tsconfig.node.json scripts/test-auth-api.ts",
    "db:test": "ts-node --project tsconfig.node.json scripts/test-db-connection.ts",
    "db:seed": "ts-node --project tsconfig.node.json scripts/seed-database.ts",
    "db:seed-test": "ts-node --project tsconfig.node.json scripts/seed-test-data.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "@mui/material": "^6.4.11",
    "@emotion/react": "^11.13.5",
